#!/bin/bash
Action=$1
C="\e[35e"
N="\e[0m"
case $Action in
    Start)
      echo -e "Starting the service"
      ;;
    Stop)
      echo -e "Stopping the service"
      ;;
     *)
        echo "Usage:: $0 start|stop"
        echo -e "\t ${C}start${N}->starting the service\n\t ${C}stop${N}->stopping the service"
  esac

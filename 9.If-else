#!/bin/bash

if [ "$action" = "start" ]; then 
    echo "Starting the service"
elif [ "$action" = "stop" ]; then 
    echo "Stopping the service"
else 
    echo "Usage:: $0 start|stop"
    echo -e "\t ${R}start${N} -> Start the service"
    echo -e "\t ${R}stop${N}  -> Stop the service"
    exit 1
fi
